{
    "chmod": "0755",
    "compression": "GZ",
    "directories": [
        "src"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "Tests",
                "tests",
                "test"
            ],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "bin/myapp",
    "output": "build/myapp.phar",
    "stub": true
}
